{
  "name": "trade_detail",
  "version": "1.0.0",
  "description": "Stock and Index Price Display App for ChatGPT",
  "type": "module",
  "scripts": {
    "dev": "tsx dev-all.mts",
    "build": "tsx build-all.mts",
    "serve": "vite preview --port 4444 --host --strictPort --config vite.host.config.mts",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "format": "prettier --write \"src/**/*.{ts,tsx}\""
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "lucide-react": "^0.460.0"
  },
  "devDependencies": {
    "@types/node": "^22.5.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.10",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.2"
  }
}